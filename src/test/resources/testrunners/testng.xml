<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="HubSpot Test Automation Regression Suite" verbose="2">

	<listeners>
		<listener
			class-name="com.qa.hubspot.listeners.ExtentReportListener" />
		<listener
			class-name="com.qa.hubspot.listeners.TestAllureListener" />
		<listener
			class-name="com.qa.hubspot.listeners.ScreenshotListener" />
	</listeners>

	<test name="Login Page Test">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.qa.hubspot.tests.LoginPageTest" />
		</classes>
	</test> <!-- Test -->

	<!-- <test name="Home Page Test"> <parameter name="browser" value="firefox" 
		/> <classes> <class name="com.qa.hubspot.tests.HomePageTest" /> </classes> 
		</test> <test name="Contacts Page Test"> <parameter name="browser" value="chrome" 
		/> <classes> <class name="com.qa.hubspot.tests.ContactsPageTest" /> </classes> 
		</test> -->

</suite> <!-- Regression Suite -->
